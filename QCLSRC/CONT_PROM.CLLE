             DCL        VAR(&ZIP)     TYPE(*CHAR) STG(*DEFINED) LEN(5)  DEFVAR(&VARRCD1  94)
             INCLUDE    SRCMBR(QUIEXITCSP)
             CHGVAR     VAR(&VARRCD_P) VALUE(%ADDRESS(&VARRCD1))

             IF         COND(&TYPCAL *EQ 8) THEN(DO)
             SELECT
