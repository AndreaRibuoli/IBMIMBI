             DCL        VAR(&VARLEN1) TYPE(*INT)  STG(*DEFINED) LEN(4)  DEFVAR(&VARRCD1  11)
             DCL        VAR(&OPT)     TYPE(*INT) STG(*DEFINED)  LEN(2)  DEFVAR(&VARRCD1  15)
