             IF         COND(&FNCREQ *EQ 0) THEN(DO)
             CHGVAR     VAR(&POSOPT) VALUE(FSLT)
             CHGVAR     VAR(&CRIT)   VALUE(NE)
             CHGVAR     VAR(&VARNM)  VALUE(OPT1)
             CHGVAR     VAR(&VARNAM) VALUE(*NONE)
